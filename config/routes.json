{
  "": {
    "controller": "Default",
    "method": "start"
  },
  "login": {
    "controller": "User",
    "method": "login"
  },
  "books/:page": {
    "controller": "Book",
    "method": "getAllWithPage",
    "params": {
      "page": "number"
    }
  },
  "books": {
    "controller": "Book",
    "method": "getAll"
  },
  "book/:id": {
    "controller": "Book",
    "method": "get",
    "params": {
      "id": "number"
    }
  },
  "books/search": {
    "controller": "Book",
    "method": "search"
  },
  "book/:id/borrow": {
    "controller": "Book",
    "method": "borrow",
    "login": true,
    "params": {
      "id": "number"
    }
  },
  "book/:id/return": {
    "controller": "Book",
    "method": "returnBook",
    "login": true,
    "params": {
      "id": "number"
    }
  },
  "customers": {
    "controller": "Customer",
    "method": "getAll"
  },
  "customer/:id": {
    "controller": "Customer",
    "method": "get",
    "params": {
      "id": "number"
    }
  },
  "my-books": {
    "controller": "Book",
    "method": "getByUser",
    "login": true
  }
}